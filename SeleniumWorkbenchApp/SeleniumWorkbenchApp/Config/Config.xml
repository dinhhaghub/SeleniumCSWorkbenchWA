	<!--<database>
		<connection>Server=.;Initial Catalog=...;Integrated Security=true;Trusted_Connection=Yes;</connection>
		<functionality>SELECT * FROM ...</functionality>
		<performance>SELECT * FROM ...</performance>
	</database>-->

