	<!-- These are for prod only -->
	<clientId></clientId>
	<tenantId></tenantId>
	<redirectUri>http://localhost</redirectUri>

	<!--Collection of testing sites and credentials-->
	<tokens bearerToken=""/>

	<!--Webapp-->
	<webapp>
		<dev>
			<url></url>
			<instanceName></instanceName>
			<webApis datalakeApi="" WorkbenchApi=""/>
		</dev>
		<qa>
			<url></url>
			<instanceName></instanceName>
			<webApis datalakeApi="" WorkbenchApi=""/>
